import java.util.Scanner;
public class Alphabet
{
	public static void main (String[] args)
	{
	 Scanner input = new Scanner(System.in);
	 System.out.print("Enter The number : ");
	 int num = input.nextInt();
	 int i=0;
	 int j=0;
	 char val='A';
	 for(i=1;i<=num;i++)
	 {
     for(j=0;j<num-i;j++)
	 {
		 System.out.print(" ");
	 }
	 for(j=1;j<=i;j++)
		{
		 	
		  System.out.print(val++ +" ");
		  
		}
		System.out.println();
		
	 }
	}
}